<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>

<table>
<tr>
<td>name</td>
<td>email</td>
<td>password</td>
</tr>
<% @posts.each do |post| %>
<tr>
	<td><%= post.title %></td>
	<td><%= post.body %></td>
	<td><%= post.latitude %></td>
	<td><%= post.longitude %></td>
	<td><%= post.user.name %>
</td>
</tr>
<% end %>
</table>