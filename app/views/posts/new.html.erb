<h1>Posts#new</h1>
<p>Find me in app/views/posts/new.html.erb</p>

<%= form_for(@post) do |f| %>
<div class="field">
<p>title</p>
<%= f.text_field :title%>
</div>

<div class="field">
<p>body</p>
<%= f.text_area :body %>
</div>


<div class="field">
<p>latitude</p>
<%= f.text_field :latitude %>
</div>


<div class="field">
<p>longitude</p>
<%= f.text_field :longitude %>
</div>

<div class="field">
<p>user</p>
<%= f.collection_select :user_id, User.all, :id, :name %>
</div>

<div class="actions">
<%= f.submit %>
</div>
<% end %>